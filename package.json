{
  "author": "C. Lewis <ctj.lewis@icloud.com>",
  "description": "Cross-platform support for easy disk caching in React Native.",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "test": "tsdx test",
    "posttest": "node test/import.mjs && node test/require.cjs"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rn-disk-cache",
  "size-limit": [
    {
      "path": "dist/rn-disk-cache.production.min.cjs",
      "limit": "10 KB"
    },
    {
      "path": "dist/rn-disk-cache.mjs",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.1",
    "husky": "^7.0.1",
    "size-limit": "^5.0.1",
    "tsdx": "ctjlewis/tsdx",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "react-native-fs": "ctjlewis/react-native-fs"
  }
}
